 <% @sort_versions.each do |version,issue|%>
  <% if !params[:select_version] || params[:select_version] && params[:select_version] == 'all' ||  params[:select_version] == 'version_working'%>
    <tr class="version_row">
  		<td colspan="8"><%= version %></td>
  	</tr>
  <% end %>
  <% issue.each do |issue|%>
    <tr class="issue_row">
          <%= render :partial => "list_issues", :locals => { :issue => issue} %>
    </tr>
  <% end %>
<% end %>


