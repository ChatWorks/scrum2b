<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
<%= javascript_include_tag 'version.js', :plugin => 'scrum2b' %>
<% content_for :header_tags do %>
    <%= stylesheet_link_tag 'version', :plugin => 'scrum2b' %>
<% end %>
<%= stylesheet_link_tag 'version', :plugin => 'scrum2b' %>
<p class="issues">
	Issues
</p>
<%=	button_to "Board",{:action => "board",:project_id =>  params[:project_id]},:name => "view_mode",:class => "btn_submit_board"%>
<%= render :partial => "filter_list", :locals => { :list_version => @list_versions} %>
<div id="version_version">
	<div class="thead clearfix">
		<img src="/images/toggle_check.png?1349682375" alt="Toggle_check">
		<p class="id_issue">
			#
		</p>
		<p class="status">
			Status
		</p>
		<p class="subject">
			Subject
		</p>
		<p class="assignee">
			Assignee
		</p>
		<p class="update">
			Updated
		</p>
		<p class="estimated">
			Estimated time
		</p>
		<p class="done">
			% Done
		</p>
	</div>
	<% if @version %>
	<%= render :partial => "version", :collection => @version %>
	<% end %>

</div>
<% if !params[:select_version] || params[:select_version] && params[:select_version] == 'backlog' || params[:select_version] == 'all'  %>
<%= render :partial => "backlog", :locals =>{:issues_backlog => @issues_backlog }%>
<% end %>




